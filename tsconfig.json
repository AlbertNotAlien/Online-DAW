{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true
  },
  "include": [
    "pages/components/AudioClip/Wavesurfer-test.jsx",
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "pages/components/AudioClip/Waveform.js",
    "pages/components/AudioClip/WaveSurferNext.jsx
    "pages/components/DragMove/index.jsx"
  ],
  "exclude": ["node_modules"]
}
